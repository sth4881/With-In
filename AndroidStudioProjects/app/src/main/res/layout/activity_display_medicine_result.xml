<?xml version="1.0" encoding="utf-8"?>
<LinearLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:orientation="vertical"
    tools:context=".DisplayMedicineResultActivity">

    <!--약 백과사전 검색 결과-->
    <TextView
        android:id="@+id/tvSearchResultTitle"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:layout_marginTop="36dp"
        android:text="약 백과사전 검색 결과"
        android:textSize="24sp"
        android:gravity="center"/>

    <ImageView
        android:id="@+id/ivMedicineImage"
        android:layout_width="match_parent"
        android:layout_height="100dp"
        android:layout_gravity="center" />

    <TextView
        android:id="@+id/tvMedicineName"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:textSize="18sp"
        android:gravity="center" />

    <TextView
        android:id="@+id/tvMedicineEffect"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:textSize="18sp"
        android:gravity="center" />

    <TextView
        android:id="@+id/tvMedicineManual"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:textSize="18sp"
        android:gravity="center" />

    <TextView
        android:id="@+id/tvMedicineWarning"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:textSize="18sp"
        android:gravity="center" />

    <TextView
        android:id="@+id/tvMedicineSideEffect"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:textSize="18sp"
        android:gravity="center" />

</LinearLayout>